<ns0:RootFileSystem xmlns:ns0="https://www.linutronix.de/projects/Elbe" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" created="2009-05-20T08:50:56" revision="6" xsi:schemaLocation="https://www.linutronix.de/projects/Elbe dbsfed.xsd">
	<project>
		<name>beaglebone-bsp</name>
		<version>1.0</version>
		<description>
			rootfs for beaglebone with real-time kernel
		</description>
		<buildtype>armel</buildtype>
		<mirror>
			<primary_host>ftp.de.debian.org</primary_host>
			<primary_path>/debian</primary_path>
			<primary_proto>http</primary_proto>
			<url-list>
				<url>
					<binary>
						http://debian.linutronix.de/elbe squeeze main
					</binary>
				</url>
			</url-list>
		</mirror>
		<noauth />
		<suite>squeeze</suite>
		<buildimage>
			<kinitrd>testrd</kinitrd>
			<portforwarding>
				<forward>
					<proto>tcp</proto>
					<buildenv>22</buildenv>
					<host>5022</host>
				</forward>
			</portforwarding>
		</buildimage>
	</project>
	<target>
		<hostname>lxbone</hostname>
		<domain>linutronix.de</domain>
		<passwd>foo</passwd>
		<console>ttyS0,115200</console>
		<images>
			<msdoshd>
				<name>sdcard.img</name>
				<size>1900MiB</size>
					<partition>
						<size>50MiB</size>
						<label>boot</label>
						<bootable />
					</partition>
					<partition>
						<size>remain</size>
						<label>rfs</label>
					</partition>
			</msdoshd>
		</images>
		<fstab>
			<bylabel>
				<label>rfs</label>
				<mountpoint>/</mountpoint>
				<fs>
					<type>ext4</type>
					<tune2fs>-i 0</tune2fs>
				</fs>
			</bylabel>
			<bylabel>
				<label>boot</label>
				<mountpoint>/boot</mountpoint>
				<fs>
					<type>vfat</type>
				</fs>
			</bylabel>
			<bydev>
				<source>none</source>
				<mountpoint>/sys/kernel/debug</mountpoint>
				<fs>
					<type>debugfs</type>
				</fs>
			</bydev>
			<bydev>
				<source>none</source>
				<mountpoint>/proc</mountpoint>
				<fs>
					<type>proc</type>
				</fs>
			</bydev>
			<bydev>
				<source>none</source>
				<mountpoint>/sys</mountpoint>
				<fs>
					<type>sysfs</type>
				</fs>
			</bydev>
		</fstab>
		<finetuning>
			<rm>var/cache/apt/archives/*.deb</rm>
			<rm>boot/vmlinuz-2.6.32-5-versatile</rm>
			<rm>boot/System.map-2.6.32-5-versatile</rm>
			<rm>boot/initrd.img-2.6.32-5-versatile</rm>
			<rm>boot/config-2.6.32-5-versatile</rm>
		</finetuning>
		<pkg-list>
			<pkg>u-boot-beaglebone</pkg>
			<pkg>linux-image-3.8.9-beaglebone-rt4</pkg>
			<pkg>build-essential</pkg>
			<pkg>gdbserver</pkg>
			<pkg>openssh-server</pkg>
			<pkg>less</pkg>
			<pkg>bash</pkg>
			<pkg>vim-nox</pkg>
			<pkg>wget</pkg>
			<pkg>aptitude-dbg</pkg>
			<pkg>dosfstools-dbg</pkg>
			<pkg>e2fslibs-dbg</pkg>
			<pkg>e2fsprogs-dbg</pkg>
			<pkg>g++-dbg</pkg>
			<pkg>isc-dhcp-client-dbg</pkg>
			<pkg>libbsd0-dbg</pkg>
			<pkg>libc6-dbg</pkg>
			<pkg>libcomerr2-dbg</pkg>
			<pkg>libcwidget3-dbg</pkg>
			<pkg>libdb4.7-dbg</pkg>
			<pkg>libdb4.8-dbg</pkg>
			<pkg>libgcc1-dbg</pkg>
			<pkg>libgomp1-dbg</pkg>
			<pkg>libncurses5-dbg</pkg>
			<pkg>libncursesw5-dbg</pkg>
			<pkg>libnfnetlink0-dbg</pkg>
			<pkg>libparted0debian1-dbg</pkg>
			<pkg>libreadline5-dbg</pkg>
			<pkg>libreadline6-dbg</pkg>
			<pkg>libruby1.8-dbg</pkg>
			<pkg>libsqlite3-0-dbg</pkg>
			<pkg>libss2-dbg</pkg>
			<pkg>libssl0.9.8-dbg</pkg>
			<pkg>libstdc++6-dbg</pkg>
			<pkg>libx11-6-dbg</pkg>
			<pkg>libxapian22-dbg</pkg>
			<pkg>libxau6-dbg</pkg>
			<pkg>libxcb1-dbg</pkg>
			<pkg>libxdmcp6-dbg</pkg>
			<pkg>libxext6-dbg</pkg>
			<pkg>libxml2-dbg</pkg>
			<pkg>libxmuu1-dbg</pkg>
			<pkg>python-dbg</pkg>
			<pkg>python-apt-dbg</pkg>
			<pkg>python2.6-dbg</pkg>
			<pkg>zlib1g-dbg</pkg>
		</pkg-list>
	</target>
