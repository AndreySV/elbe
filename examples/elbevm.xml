<ns0:RootFileSystem xmlns:ns0="https://www.linutronix.de/projects/Elbe" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" created="2009-05-20T08:50:56" revision="6" xsi:schemaLocation="https://www.linutronix.de/projects/Elbe dbsfed.xsd">
  <project>
    <name>elbe-devel-vm-</name>
    <version>1.0</version>
    <description>amd64 debian jessie VM for E.L.B.E. demo</description>
    <buildtype>amd64</buildtype>
    <mirror>
      <primary_host>ftp.de.debian.org</primary_host>
      <primary_path>/debian</primary_path>
      <primary_proto>http</primary_proto>
      <url-list>
	<url>
	  <spdx>http://debian.linutronix.de/spdx</spdx>
	</url>
      </url-list>
    </mirror>
    <suite>jessie</suite>
  </project>
  <target>
    <hostname>elbevm</hostname>
    <domain></domain>
    <passwd>foo</passwd>
    <console>ttyS0,115200</console>
    <images>
      <msdoshd>
	<name>elbevm.img</name>
	<size>50000MiB</size>
	<grub-install/>
	<partition>
	  <size>remain</size>
	  <label>rfs</label>
	</partition>
      </msdoshd>
    </images>
    <fstab>
      <bylabel>
	<label>rfs</label>
	<mountpoint>/</mountpoint>
	<fs>
	  <type>ext4</type>
	  <tune2fs>-i 0</tune2fs>
	</fs>
      </bylabel>
    </fstab>
    <finetuning>
      <rm>var/cache/apt/archives/*.deb</rm>
    </finetuning>
    <pkg-list>
      <pkg>linux-image-amd64</pkg>
      <pkg>grub-pc</pkg>
      <pkg>dh-python</pkg>
      <pkg>debhelper</pkg>
      <pkg>python</pkg>
      <pkg>python-mako</pkg>
      <pkg>python-lxml</pkg>
      <pkg>dia</pkg>
      <pkg>asciidoc</pkg>
      <pkg>xmlto</pkg>
      <pkg>lintian</pkg>
      <pkg>source-highlight</pkg>
      <pkg>git</pkg>
    </pkg-list>
  </target>
</ns0:RootFileSystem>
